        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "\n",
        "# Load the uploaded Excel file\n",
        "df = pd.read_excel('FEV-data-Excel.xlsx')\n",
        "\n",
        "# View basic info\n",
        "df.head()\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "# TASK 1\n",
        "\n",
        "# a) Filter EVs within budget and range\n",
        "filtered_ev = df[(df['Minimal price (gross) [PLN]'] <= 350000) &\n",
        "             (df['Range (WLTP) [km]'] >= 400)]\n",
        "print(\"Filtered EV: \",filtered_ev)\n",
        "\n",
        "# b) Group by manufacturer\n",
        "grouped_by_make = filtered_ev.groupby('Make')\n",
        "\n",
        "# c) Average battery capacity per manufacturer\n",
        "avg_battery = grouped_by_make['Battery capacity [kWh]'].mean()\n",
        "\n",
        "# Display result\n",
        "avg_battery.sort_values(ascending=False)\n",
        "\n",
        "# EXPLAINATION\n",
        "#We filtered EVs with a price ≤ 350000 PLN and range ≥ 400 km.\n",
        "#Then we grouped by manufacturer and calculated the average battery capacity.\n"
      ],
