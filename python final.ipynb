        "# Task 4: EV Recommendation Class\n",
        "\n",
        "class EVRecommender:\n",
        "    def __init__(self, dataset):\n",
        "        self.data = dataset.dropna(subset=[\n",
        "            'Minimal price (gross) [PLN]',\n",
        "            'Range (WLTP) [km]',\n",
        "            'Battery capacity [kWh]'\n",
        "        ])\n",
        "\n",
        "    def recommend(self, budget, min_range, min_battery):\n",
        "        # Filter based on input of user\n",
        "        filtered = self.data[\n",
        "            (self.data['Minimal price (gross) [PLN]'] <= budget) &\n",
        "            (self.data['Range (WLTP) [km]'] >= min_range) &\n",
        "            (self.data['Battery capacity [kWh]'] >= min_battery)\n",
        "        ]\n",
        "\n",
        "        # Sort by range (you can change to price or battery if needed)\n",
        "        top_ev = filtered.sort_values(by='Range (WLTP) [km]', ascending=False).head(3)\n",
        "\n",
        "        return top_ev[['Car full name', 'Minimal price (gross) [PLN]',\n",
        "                       'Range (WLTP) [km]', 'Battery capacity [kWh]']]\n",
        "\n",
        "# Create an object\n",
        "obj1 = EVRecommender(df)\n",
        "\n",
        "# Call the recommend method (example)\n",
        "obj1.recommend(budget=500000, min_range=500, min_battery=65)\n",
        "\n",
        "\n",
        "# EXPLAINATION\n",
        "#Created a Python class called EVRecommender.\n",
        "#Inside the class, i have used the __init__ method to load and clean the data.\n",
        "#Then i defined a method recommend() that:\n",
        "#Accepts 3 inputs from the user: budget, minimum range, and minimum battery capacity.\n",
        "#Filters the dataset using these conditions.\n",
        "#Sorts the results by range and shows the top 3 matching EVs.\n",
        "#Then created an object of the class called 'obj1' and tested it by passing sample values.\n"
